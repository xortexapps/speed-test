<?xml version="1.0" encoding="UTF-8"?>
<widget xmlns     = "http://www.w3.org/ns/widgets"
        xmlns:gap = "http://phonegap.com/ns/1.0"
        id        = "com.xortexebusinessgmbh.speedtest"
        version   = "0.0.1">
    <name>Speed Test</name>

    <description>
        Speed Test
    </description>

    <author href="http://www.xortex.at" email="apps@xortex.com">
        XORTEX eBusiness GmbH
    </author>

    <feature name="http://api.phonegap.com/1.0/device" />

    <preference name="phonegap-version" value="2.3.0" />
    <preference name="orientation"      value="default" />
    <preference name="target-device"    value="universal" />
    <preference name="fullscreen"       value="false" />

    <icon src="icon.png" />
    <icon src="phoneGapRes/icon/android/icon-36-ldpi.png"   gap:platform="android"    gap:density="ldpi" />
    <icon src="phoneGapRes/icon/android/icon-48-mdpi.png"   gap:platform="android"    gap:density="mdpi" />
    <icon src="phoneGapRes/icon/android/icon-72-hdpi.png"   gap:platform="android"    gap:density="hdpi" />
    <icon src="phoneGapRes/icon/android/icon-96-xhdpi.png"  gap:platform="android"    gap:density="xhdpi" />
    <icon src="phoneGapRes/icon/blackberry/icon-80.png"     gap:platform="blackberry" />
    <icon src="phoneGapRes/icon/blackberry/icon-80.png"     gap:platform="blackberry" gap:state="hover"/>
    <icon src="phoneGapRes/icon/ios/icon-57.png"            gap:platform="ios"        width="57" height="57" />
    <icon src="phoneGapRes/icon/ios/icon-72.png"            gap:platform="ios"        width="72" height="72" />
    <icon src="phoneGapRes/icon/ios/icon-57-2x.png"         gap:platform="ios"        width="114" height="114" />
    <icon src="phoneGapRes/icon/ios/icon-72-2x.png"         gap:platform="ios"        width="144" height="144" />
    <icon src="phoneGapRes/icon/webos/icon-64.png"          gap:platform="webos" />
    <icon src="phoneGapRes/icon/windows-phone/icon-48.png"  gap:platform="winphone" />
    <icon src="phoneGapRes/icon/windows-phone/icon-173.png" gap:platform="winphone"   gap:role="background" />

    <gap:splash src="phoneGapRes/screen/android/screen-ldpi-portrait.png"  gap:platform="android" gap:density="ldpi" />
    <gap:splash src="phoneGapRes/screen/android/screen-mdpi-portrait.png"  gap:platform="android" gap:density="mdpi" />
    <gap:splash src="phoneGapRes/screen/android/screen-hdpi-portrait.png"  gap:platform="android" gap:density="hdpi" />
    <gap:splash src="phoneGapRes/screen/android/screen-xhdpi-portrait.png" gap:platform="android" gap:density="xhdpi" />
    <gap:splash src="phoneGapRes/screen/blackberry/screen-225.png"         gap:platform="blackberry" />
    <gap:splash src="phoneGapRes/screen/ios/screen-iphone-portrait.png"    gap:platform="ios"     width="320" height="480" />
    <gap:splash src="phoneGapRes/screen/ios/screen-iphone-portrait-2x.png" gap:platform="ios"     width="640" height="960" />
    <gap:splash src="phoneGapRes/screen/ios/screen-ipad-portrait.png"      gap:platform="ios"     width="768" height="1024" />
    <gap:splash src="phoneGapRes/screen/ios/screen-ipad-landscape.png"     gap:platform="ios"     width="1024" height="768" />
    <gap:splash src="phoneGapRes/screen/windows-phone/screen-portrait.jpg" gap:platform="winphone" />
</widget>
